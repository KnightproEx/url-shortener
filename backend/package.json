  },
  "dependencies": {
    "@elysiajs/bearer": "^1.1.2",
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/swagger": "^1.1.6",
    "@prisma/client": "^6.0.0",
    "elysia": "^1.1.25",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/jsonwebtoken": "^9.0.7",
    "bun-types": "^1.1.38",
    "prisma": "^6.0.0"
  }
}
